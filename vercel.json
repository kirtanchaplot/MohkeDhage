{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "https://mohkedhage.onrender.com/api/$1" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }