{
    "version": 2,
    "builds": [
      { 
        "src": "frontend/package.json", 
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "https://mohkedhage.onrender.com/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/dist/$1" }
    ]
  }